@Article{Moore1996,
  Title                    = {A geometric basis for measurement of three-dimensional eye position using image processing.},
  Author                   = {S. T. Moore and T. Haslwanter and I. S. Curthoys and S. T. Smith},
  Journal                  = {Vision Res},
  Year                     = {1996},

  Month                    = {Feb},
